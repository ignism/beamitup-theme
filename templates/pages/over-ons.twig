{% extends "base.twig" %}

{% block content %}

    <header>
        {% include "components/block.twig" with {
            orientation: 'landscape-double',
            color: 'grey-dark'
        } %}
    </header>

    <section class="w-full" style="transform: translate(0, -10%)">
        <div class="flex flex-wrap -mx-5">

            <div class="w-6/12 ml-5/12">

                {% include "partials/text-block.twig" with {
                    title: 'Visie &amp;<br>missie',
                    color: 'blue',
                    background: 'yellow'
                } %}

            </div>

        </div>
    </section>

    <section class="w-full my-32">
        <div class="flex flex-wrap -mx-5">

            <div class="w-6/12 ml-1/12">

                {% include "partials/text-block.twig" with {
                    title: 'Visie &amp;<br>missie',
                    color: 'blue',
                    background: 'yellow'
                } %}

            </div>

        </div>

        <div style="transform: translate(0, -40%)">
            {% include "components/block.twig" with {
                orientation: 'landscape-double',
                color: 'grey-dark'
            } %}
        </div>
    </section>

    <section class="w-full my-32 relative">
        <div class="absolute pin w-full">
            {% include "components/block.twig" with {
                orientation: 'landscape-triple',
                color: 'yellow'
            } %}
        </div>

        <div class="flex flex-wrap -mx-5 pt-16">
            <div class="w-10/12 ml-1/12">
                <h1 class="highlight-wrapper text-5xl mb-10" data-color="yellow" data-background="blue">
                    Ons team
                </h1>
            </div>
        </div>
        <div class="flex flex-wrap -mx-5 text-2xl">
            <div class="w-4/12 ml-1/12">
                {% include "components/member.twig" with {
                    color : "grey",
                    name: "Robert Storm",
                    role: "Educatief Ontwerper"
                } %}
            </div>
            <div class="w-4/12 ml-2/12 mt-1/12">
                {% include "components/member.twig" with {
                    color : "grey",
                    name: "Liselotte de groot",
                    role: "Educatief Ontwerper"
                } %}
            </div>
        </div>

    </section>

    <section class="w-full my-32">
        <div class="relative w-full">
            <div class="flex flex-wrap -mx-5 text-2xl">
                <div class="w-6/12 ml-1/12 flex flex-wrap">
                    <div class="w-6/12 ml-2/12">

                        {% include "components/member.twig" with {
                            color : "grey",
                            name: "Karlijn de groot",
                            role: "Workshop docent"
                        } %}

                    </div>
                    <div class="w-6/12 ml-4/12 mt-1/12">

                        {% include "components/member.twig" with {
                            color : "grey",
                            name: "Linda Jansen",
                            role: "Stagiaire"
                        } %}

                    </div>
                </div>
                <div class="w-4/12 text-3xl">
                    Wij zijn:
                    <p class="highlight-wrapper" data-color="blue" data-background="yellow">
                        optimistisch<br>
                        ongecompliceerd<br>
                        sympathiek<br>
                        sociaal<br>
                        zelfstandig<br>
                        zelfvoorzienend<br>
                        enthousiast<br>
                        relaxt<br>
                        snel<br>
                        actief<br>
                        praktisch<br>
                        ...<br>
                        Beam it Up
                    </p>
                </div>
            </div>
        </div>
    </section>

    <section class="w-full my-32">
        <div class="relative w-full">
            {% include "components/block.twig" with {
                orientation: 'landscape-triple',
                color: 'blue-darker'
            } %}
        </div>
        <div style="margin-top: -25%">
            <div class="flex flex-wrap -mx-5">
                <div class="w-10/12 ml-1/12">
                    <h1 class="highlight-wrapper text-5xl z-20 relative" data-color="blue-darker" data-background="pink-lighter" style="transform: translate(0, 25%)">
                        Wij werken<br>graag samen met
                    </h1>
                </div>
            </div>
            <div class="flex flex-wrap -mx-5 text-2xl">
                <div class="w-10/12 ml-1/12 p-10 bg-white z-10">

                    {% include "components/block.twig" with {
                        orientation: 'landscape',
                        color: 'white',
                        text: 'logo\'s'
                    } %}

                </div>
            </div>
        </div>
    </section>

    <section class="w-full my-32">
        {% include "components/slider.twig" with {
            slides : [
                {
                    text: "Greenscreen<br>workshop"
                }, {
                    text: "2"
                }, {
                    text: "3"
                }
            ],
            width: "100%",
            height: "50vw",
            pagination: true
        } %}
    </section>

    <section class="w-full my-32">
        <div class="flex flex-wrap -mx-5 text-xl">
            <div class="px-5 ml-1/12 w-10/12">

                <div class="block-outer">
                    <div class="block-inner landscape-double">
                        <div class="content bg-yellow flex items-center justify-center" style="background-image: url()"></div>
                    </div>
                </div>

            </div>
        </div>
    </section>


{% endblock %}
